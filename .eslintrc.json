{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "@typescript-eslint"
    ],
    "rules": {
        "no-console": "off",
        "no-plusplus": "off",
        "no-empty": "off",
        "no-constant-condition": "off",
        "no-underscore-dangle": "off",
        "no-unused-vars": "warn",
        "no-unused-expressions": "off"
    }
}
